{
  "name": "npm-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "preferGlobal": false,
  "author": "Michael Scott Hertzberg <mshertzberg@gmail.com> (http://hertzber.gs/)",
  "license": "MIT",
  "bin": {
    "npm-boilerplate": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moimikey/npm-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/moimikey/npm-boilerplate/issues"
  },
  "homepage": "https://github.com/moimikey/npm-boilerplate",
  "keywords": [
    ""
  ],
  "scripts": {
    "build": "babel --compact --ignore *.spec.js -d lib src",
    "clean": "rimraf .eslintcache *.log lib/**/*.{js,log}",
    "lint": "eslint --cache src benchmark",
    "prebuild": "npm-run-all --sequential lint clean",
    "prepublishOnly": "npm-run-all --sequential test build",
    "test": "babel-tap --bail --no-color --reporter=spec src/**/*.spec.js",
    "test:dev": "babel-tap --bail src/**/*.spec.js",
    "test:watch": "nodemon -w src --exec npm run test:dev"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-tap": "5.0.0",
    "capture-stream": "0.1.2",
    "eslint": "3.13.1",
    "eslint-plugin-babel": "4.0.0",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.0",
    "rimraf": "2.5.4",
    "tap": "9.0.3",
    "to-factory": "1.0.0",
    "typeov": "1.2.4"
  },
  "dependencies": {
    "babel-runtime": "6.20.0"
  }
}
